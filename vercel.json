{
  "buildCommand": "echo 'Static HTML site'",
  "outputDirectory": "./",
  "installCommand": "npm install",
  "framework": null,
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/megathread.html",
      "destination": "/megathread",
      "permanent": true
    },
    {
      "source": "/library.html",
      "destination": "/library",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/megathread",
      "destination": "/megathread.html"
    },
    {
      "source": "/library",
      "destination": "/library.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/((?!megathread|library).*)",
      "destination": "/index.html"
    }
  ]
}
